+
+		if (ret) {
+			misp_err("%s - step4. probe - request irq error",
+				__func__);
+			goto err_dev_attr;
+		}
+		misp_info("%s - step4 done. irq number:%d", __func__,
+			drv_global_variable->irq_num);
+
+		free_irq(drv_global_variable->irq_num, drv_data);
+	}
+
