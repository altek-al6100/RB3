+    f = filp_open(filename, O_APPEND | O_CREAT | O_RDWR, 0777);
+    /*Get current segment descriptor*/
+    fs = get_fs();
+    /*Set segment descriptor associated*/
+    set_fs(get_ds());
+    /*write the file*/
+    f->f_op->write(f, (char *)keep_dump_memory, ouput_size, &f->f_pos);
